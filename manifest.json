
{
  "manifest_version": 3,
  "name": "Linked Job Dimmer",
  "version": "1.1",
  "description": "Dims or hides previously viewed or applied jobs on LinkedIn.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "options.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://www.linkedin.com/jobs/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  }
}
