{
  "manifest_version": 3,
  "name": "LinkedIn Job Dimmer",
  "version": "1.1",
  "description": "Darkens LinkedIn job cards marked as Viewed or Applied, with customizable options.",
  "permissions": ["storage"],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  }
}
